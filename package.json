{
	"name": "npm-package-demo",
	"version": "1.0.1",
	"description": "A demo package ",
	"keywords": ["demo", "typescript"],
	"homepage": "https://github.com/letanure/npm-package-demo",
	"bugs": {
		"url": "https://github.com/letanure/npm-package-demo/issues"
	},
	"author": "Luiz Tanure",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/letanure/npm-package-demo.git"
	},
	"license": "MIT",
	"type": "module",
	"files": ["dist"],
	"main": "dist/index.js",
	"scripts": {
		"build": "tsc",
		"ci": "npm run build && npm run lint:all && npm run test:all",
		"lint:all": "npm run lint:exports && npm run lint:format && npm run lint:ts",
		"lint:exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
		"lint:format:fix": "biome format --write .",
		"lint:format": "biome check",
		"lint:ts": "tsc --noEmit",
		"prepublishOnly": "npm run ci",
		"release:local": "changeset version && changeset publish",
		"test:all": "npm run test:run",
		"test:run": "vitest run",
		"test:watch": "vitest"
	},
	"devDependencies": {
		"@arethetypeswrong/cli": "^0.15.4",
		"@biomejs/biome": "1.8.3",
		"@changesets/cli": "^2.27.7",
		"typescript": "^5.5.4",
		"vitest": "^2.0.5"
	}
}
