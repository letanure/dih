{
	"name": "tt-package-demo",
	"version": "1.0.0",
	"description": "A demo package ",
	"keywords": [
		"demo",
		"typescript"
	],
	"homepage": "https://github.com/letanure/tt-package-demo",
	"bugs": {
		"url": "https://github.com/letanure/tt-package-demo/issues"
	},
	"author": "Luiz Tanure",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/letanure/tt-package-demo.git"
	},
	"license": "MIT",
	"type": "module",
	"files": [
		"dist"
	],
	"main": "dist/index.js",
	"scripts": {
		"build": "tsc",
		"ci": "npm run build && npm run lint:check && npm run lint:exports && npm run lint:ts && npm run test:run",
		"lint:ts": "tsc",
		"lint:check": "biome check",
		"lint:exports": "attw --pack .",
		"lint:fix": "biome format --write .",
		"test:run": "vitest run",
		"test:watch": "vitest",
		"release:local": "changeset version && changeset publish"
	},
	"devDependencies": {
		"@arethetypeswrong/cli": "^0.15.4",
		"@biomejs/biome": "1.8.3",
		"@changesets/cli": "^2.27.7",
		"typescript": "^5.5.4",
		"vitest": "^2.0.5"
	}
}
